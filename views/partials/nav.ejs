  <% if (!user) { %>
    <link rel="stylesheet" href="/stylesheets/siwg.css">
  <% } %>
</head>
<body>
  <nav>
    <% if (user) { %>

        <form id="back-btn"
        action="/cars"
        method="get">
          <button id="back-button" class="btn btn-dark" type="submit">Back</button>
        </form>
  
        <form id="login-btn"
        action="/auth/logout"
        method="get">
          <button id="login-button" class="btn btn-warning" type="submit">Logout</button>
        </form>

    <% } else { %>
      <a href="/auth/google" class="siwg-link">
        <button type="submit" class="siwg">
          <img id="siwg-image" alt="Sign in with Google" />
        </button>
      </a>
    <% } %>
  </nav>
